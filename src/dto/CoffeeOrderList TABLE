CREATE TABLE COFFEE_ORDER_LIST(
	NAME VARCHAR2(50) PRIMARY KEY,
	SYRUP VARCHAR2(20) NOT NULL,
	ORDERSIZE VARCHAR2(20) NOT NULL,
	SHOT NUMBER(1) NOT NULL,
	CREAM NUMBER(1) NOT NULL,
	ORDERCOUNT NUMBER(5) NOT NULL,
	PRICE NUMBER(12) NOT NULL,
	ORDERDATE DATE NOT NULL
);

DROP TABLE COFFEE_ORDER_LIST;

SELECT *
FROM COFFEE_ORDER_LIST;

insert into COFFEE_ORDER_LIST
values();

	


